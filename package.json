{
  "name": "react-date-range-dayjs",
  "version": "1.1.3",
  "description": "A React component for choosing dates and date ranges based on react-date-range but using Dayjs as date manager.",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn build:css & styleguidist server",
    "build": "yarn build:css & yarn build:js & styleguidist build",
    "build:css": "postcss 'src/styles.scss' -d dist --ext css & postcss 'src/theme/*.scss' -d 'dist/theme' --ext css",
    "build:js": "babel ./src --out-dir ./dist --ignore test.js",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "test": "jest",
    "clear": "rm -rf dist",
    "preversion": "yarn clear & yarn build",
    "prepack": "yarn clear & yarn build"
  },
  "keywords": [
    "react",
    "date",
    "range",
    "datepicker",
    "rangepicker",
    "dayjs"
  ],
  "contributors": [
    "Henry Lagarde (https://github.com/tolares)",
    "Daniel Flores (https://github.com/devniel)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tolares/react-date-range-dayjs"
  },
  "bugs": {
    "url": "https://github.com/tolares/react-date-range-dayjs/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react-list": "^0.8.13",
    "shallow-equal": "^1.2.1"
  },
  "peerDependencies": {
    "dayjs": "^1.11.7",
    "react": "^0.14 || ^15.0.0-rc || >=15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dayjs": "^1.11.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.2.13",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^10.4.0",
    "style-loader": "^1.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5"
  }
}
